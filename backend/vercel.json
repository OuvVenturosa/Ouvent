{
  "version": 2,
  "builds": [
    {
      "src": "backend.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend.js"
    },
    {
      "src": "/(.*)",
      "dest": "/backend.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DB_PATH": "./database/ouvidoria.db"
  }
}
